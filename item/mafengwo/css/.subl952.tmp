html,body,div,span,ul,li,a,h2{margin:0;padding:0;}
li{list-style: none;}
a{text-decoration:none;}
img{vertical-align:middle;}
input{border:0;outline: none;}

.topbar{}
.topbar-content{width: 1000px;margin:auto;/*border:1px solid red;*/;height:66px;}
.topbar-logo{float:left;/*border:1px solid red;*/display:inline-block;margin:10px 0;}
.topbar-logo:after{content:' ';display:block;clear:both;}
.topbar-logo span{display: inline-block;width: 136px;height: 39px;
	background:url(../images/sprite.png)no-repeat 0 5px;}
.topbar-logo span:hover{transform: rotateY(360deg);transition: all 1s;}
.topbar-list{float:left;/*border:1px solid red;*/padding-left:25px;line-height: 68px;border-bottom:2px sol}
.topbar-list:after{content:' ';display:block;clear:both;}
.topbar-list ul{id #fff;}
.topbar-list ul li{float:left;display:inline-block;}
.topbar-list ul li:hover{border-bottom:2px solid rgb(255,158,0);}
.topbar-list li a{color:#000;display:inline-block;padding:0 16px;}
.topbar-list li a:hover{color:rgb(255,158,0);}
.topbar-list li:first-child a{background:rgb(255,158,0);color:#fff;}
.topbar-info{float: right;display:inline-block;margin:20px 0;}
.topbar-info:after{content:' ';display:block;clear:both;}
.topbar-info a{display:inline-block;font-size:14px;color:rgb(255,158,0);margin-right:6px;}
.topbar-info a:nth-child(1),.topbar-info a:nth-child(2),.topbar-info a:nth-of-type(3){width: 26px;height: 26px;
background:url(../images/header-sprites15.png)no-repeat;vertical-align:-7px;text-indent:100px;overflow: hidden;}
.topbar-info a:hover{text-decoration:underline;}
.topbar-info a:nth-child(1){background-position:0 -50px;}
.topbar-info a:nth-child(1):hover{background-position:-0px -80px;transform: rotateY(360deg);transition:all 1.2s;}
.topbar-info a:nth-child(2){background-position:-30px -50px;}
.topbar-info a:nth-child(2):hover{background-position:-30px -80px;transform: rotateY(360deg);transition:all 1.2s;}
.topbar-info a:nth-child(3){background-position:0 -165px;}
.topbar-info a:nth-child(3):hover{background-position:-30px -165px;transform: rotateY(360deg);transition:all 1.2s;}
.topbar-info span{color:rgb(255,158,0);}

.banner{position:relative;}
.banner-bg{}
.banner-bg a{width: 100%;height: 640px;background:url(../images/wKgED1u98wKACof1AAm5Nt1lQFY04.jpeg)no-repeat;display:inline-block;}
.banner-title{position: absolute;left:450px;top:20px;}
.banner-title span:first-child h2 strong{font-weight:normal;font-size:32px;}
.banner-title span h2{color:#fff;font-weight: normal;font-size:26px;text-shadow:1px 0px 2px #000;}
.banner-nav{position:absolute;left:634px;top:480px;width: 632px;height: 120px;background: rgba(0,0,0,0.7);
			border-radius:10px;background:blue;}
.banner-nav span{color:#fff;display:block;margin-top:15px;margin-left: 15px;}
.banner-nav span label{margin-right: 20px;font-size:14px;}
.banner-nav span:nth-of-type(1)>label>input{width:16px;height: 16px;vertical-align:-4px;}
.banner-nav span:nth-of-type(2)>input{width: 506px;height:46px;padding:0 20px;
color:rgb(133,133,133);border-radius:4px;font-size:16px;}
.banner-nav span:nth-of-type(2)>a{display:inline-block;background:rgb(255,158,0);width:50px;height:46px;border-radius:4px;
vertical-align:-18px;position:relative;}
.seach{display:block;background:url(../images/header-sprites13.png)no-repeat 0px -110px;
	   width:24px;height:24px;position:absolute;left:13px;top:13px;}
.banner-nav >p{color:#fff;margin-top:40px;background:red;}
.banner-nav>p>a{color:rgb(255,158,0);font-size:14px;}
.banner-nav>p>a:hover{text-decoration:underline;}











